{
  "name": "one-click-deploy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:worker\"",
    "build": "tsc",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm run:start:*"
  },
  "dependencies": {
    "@vendure/admin-ui-plugin": "^2.2.0",
    "@vendure/asset-server-plugin": "^2.2.0",
    "@vendure/cli": "^2.2.0",
    "@vendure/core": "^2.2.0",
    "@vendure/create": "^2.2.0",
    "@vendure/email-plugin": "^2.2.0",
    "@vendure/payments-plugin": "^2.2.0",
    "dotenv": "16.3.1",
    "fs-extra": "^11.2.0",
    "pg": "8.11.5"
  },
  "devDependencies": {
    "concurrently": "8.2.2",
    "ts-node": "10.9.2",
    "typescript": "^4.9.5"
  }
}
